module ModificationEasyCustomFieldMonths
  module IssuePatch

    def self.included(base)
      base.extend(ClassMethods)
      base.send(:include, InstanceMethods)

      base.class_eval do

        alias_method_chain :to_s, :modification_easy_custom_field_months

      end
    end

    module InstanceMethods

      def to_s_with_modification_easy_custom_field_months
        'issue'
      end

    end

    module ClassMethods

    end

  end

end
RedmineExtensions::PatchManager.register_model_patch 'Issue', 'ModificationEasyCustomFieldMonths::IssuePatch'
